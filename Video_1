        "import tensorflow as tf\n",
        "import numpy as np"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "celsius = np.array([-40, -10, 0, 8, 15, 22, 38], dtype=float)  #Entradas\n",
        "fahrenheit = np.array([-40, 14, 32, 46, 59, 72, 100], dtype=float)  #Salidas"
      ],
      "metadata": {
        "id": "9_iXRF6722f0"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "capa = tf.keras.layers.Dense(units=1, input_shape=[1])  #Capa tipo Densa, 1 neurona hacia todas las neuronas de la sigueinte capa\n",
        "modelo = tf.keras.Sequential([capa])"
      ],
      "metadata": {
        "id": "tbFqgBAR3B-z"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "modelo.compile(\n",
        "    optimizer=tf.keras.optimizers.Adam(0.1),     # Adam saber como ajustar pesos y sesgos, para que aprenda\n",
        "    loss='mean_squared_error'                    # perdida,\n",
        ")"
      ],
      "metadata": {
        "id": "hlPfDk2N4jWe"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "print(\"Comenzando entrenamiento...\")\n",
        "historial = modelo.fit(celsius, fahrenheit, epochs=1000, verbose=False) #Entrada Celcius, Salida Farenheit, entrenamiento 1000, False no salga basura\n",
        "print(\"Modelo entrenado!\")"
      ],
